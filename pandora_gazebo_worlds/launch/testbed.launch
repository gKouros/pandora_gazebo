<launch>
	
	<arg name="gui" default="true"/>
	
	<include file="$(find pandora_gazebo_worlds)/launch/start.launch">
	<arg name="world" value="testbed"/>
	<arg name="gui" value="$(arg gui)"/> 
	</include>
	
	<include file="$(find pandora_gazebo_worlds)/launch/pioneer3at_static_transforms.launch" />
	
	<node name="spawn_robot" pkg="gazebo_ros" type="spawn_model" args="-file $(find pandora_gazebo_worlds)/models/pioneer3at_robot/model.sdf -sdf -y 10 -z 0.5 -model pioneer3at_robot" />
	
</launch>
