<launch>

  <arg name="gui" default="true"/>
  <arg name="robot" default="woody"/>
  
  <arg name="ros_control" default="true"/>
  <arg name="tune" default="false" />
  
  <include file="$(find pandora_gazebo_worlds)/launch/start.launch">
    <arg name="world" value="materials_test_world"/>
    <arg name="robot" value="$(arg robot)"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="z" value="0.5"/>
    <arg name="ros_control" value="$(arg ros_control)"/>
    <arg name="tune" value="$(arg tune)" />
  </include>

</launch>
