<?xml version="1.0" ?>

<robot xmlns:xacro="http://ros.org/wiki/xacro" >

  <gazebo>
    <joint name="differential_left_pin_parallel_joint"
           type="revolute" >
      <parent>differential_left_pin_link</parent>
      <child>left_side_link</child>
      <pose>0.0 0.0 -0.06533 0.0 0.0 0.0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.785</lower>
          <upper>0.785</upper>
        </limit>
        <dynamics>
          <friction>0</friction>
          <damping>10</damping>
        </dynamics>
      </axis>
    </joint>
  </gazebo>

  <gazebo>
    <joint name="differential_right_pin_parallel_joint"
           type="revolute" >
      <parent>differential_right_pin_link</parent>
      <child>right_side_link</child>
      <pose>0.0 0.0 -0.06533 0.0 0.0 0.0</pose>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.785</lower>
          <upper>0.785</upper>
        </limit>
        <dynamics>
          <friction>0</friction>
          <damping>10</damping>
        </dynamics>
      </axis>
    </joint>
  </gazebo>

  <gazebo reference="linear_actuator_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

  <gazebo reference="left_front_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
  </gazebo>
  <gazebo reference="left_rear_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
  </gazebo>
  <gazebo reference="right_front_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
  </gazebo>
  <gazebo reference="right_rear_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
  </gazebo>

  <gazebo reference="left_side_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="right_side_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

  <gazebo reference="laser_roll_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="laser_pitch_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

  <gazebo reference="kinect_pitch_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="kinect_yaw_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

</robot>
