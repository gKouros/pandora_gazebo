<?xml version="1.0" ?>

<robot xmlns:xacro="http://ros.org/wiki/xacro" >

  <gazebo reference="left_front_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
    <gravity>true</gravity>
  </gazebo>

  <gazebo reference="left_rear_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
    <gravity>true</gravity>
  </gazebo>

  <gazebo reference="right_front_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
    <gravity>true</gravity>
  </gazebo>

  <gazebo reference="right_rear_wheel_link" >
    <minDepth>${wheel_min_depth}</minDepth>
    <mu1>${wheel_mu_1}</mu1>
    <mu2>${wheel_mu_2}</mu2>
    <fdir1>1 0 0</fdir1>
    <kp>${wheel_k_p}</kp>
    <kd>${wheel_k_d}</kd>
    <maxContacts>${wheel_max_contacts}</maxContacts>
    <gravity>true</gravity>
  </gazebo>

  <gazebo reference="left_front_wheel_steer_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="left_rear_wheel_steer_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="right_front_wheel_steer_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="right_rear_wheel_steer_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

  <gazebo reference="front_virtual_suspension_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="rear_virtual_suspension_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="front_axle_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="rear_axle_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

  <gazebo reference="laser_roll_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>
  <gazebo reference="laser_pitch_joint" >
    <implicitSpringDamper>true</implicitSpringDamper>
  </gazebo>

</robot>
